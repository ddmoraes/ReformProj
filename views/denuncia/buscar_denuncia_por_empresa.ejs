
<div class="content3"></div>
<%- include('../partials/header') %>
<%- include('../partials/navbar3') %>


<h1>Buscar Denuncias</h1>
<form action="/buscarDenunciaPorEmpresa" method="POST">
    <label for="empresaId">Selecione a Empresa:</label>
    <select name="empresaId" id="empresaId">
        <% empresas.forEach(function(empresa) { %>
            <option value="<%= empresa.id %>"><%= empresa.nome %></option>
        <% }); %>
    </select>
    <button type="submit">Buscar Denuncias</button>
</form>
